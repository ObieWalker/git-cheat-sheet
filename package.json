  "scripts": {
    "client": "npm run start:client",
    "start:client": "react-scripts start",
    "start": "babel-node server/app.js",
    "prebuild": "rm -rf build",
    "heroku-postbuild": "npm run build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "nodemon --exec babel-node server/app.js",
    "seed": "babel-node server/seed/mainSeed.js",
    "test:client": "BABEL_ENV=test jest --config ./jest.config.json",
    "test:seed": "babel-node server/tests/testSeed/testSeed.js",
    "test:server": "npm run test:seed && export NODE_ENV=test && mocha --compilers js:@babel/register ./server/tests/index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
